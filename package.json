{
  "name": "@radix-namespace/typescript-sdk",
  "version": "1.0.0",
  "description": "Typescript toolkit for the Radix Namespace",
  "type": "module",
  "scripts": {
    "test": "jest",
    "dev": "nodemon",
    "start": "node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node ./src/index.ts",
    "build": "tsup",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "@radixdlt/babylon-gateway-api-sdk": "^1.10.1",
    "bech32": "^2.0.0",
    "decimal.js": "^10.6.0"
  },
  "devDependencies": {
    "@radixdlt/radix-dapp-toolkit": "^2.1.1",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3"
  }
}
